# 应用配置
NEXT_PUBLIC_APP_NAME=Luxaura
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=https://luxaura.com.au

# 环境
NODE_ENV=development

# Web3Forms 表单服务（联系表单和报价表单）
NEXT_PUBLIC_WEB3FORMS_KEY=your_web3forms_access_key_here

# API配置（如果需要）
# NEXT_PUBLIC_API_URL=https://api.example.com
# API_SECRET_KEY=your_secret_key_here

# 邮件服务配置（如果需要联系表单功能）
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASS=your_password
# CONTACT_EMAIL=contact@luxaura.com

# 分析工具（可选）
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_GTM_ID=GTM-XXXXXXX

# 其他第三方服务
# NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_site_key
# RECAPTCHA_SECRET_KEY=your_secret_key

# 数据库配置（如果需要）
# DATABASE_URL=postgresql://user:password@localhost:5432/luxaura

